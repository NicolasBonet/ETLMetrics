<?xml version="1.0" encoding="ASCII"?>
<crudGenerator:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:crudGenerator="http://www.example.org/crudGenerator"
    name="ejemplo">
  <entities
      name="Estudiante"
      associationAsTarget="//@entities.1/@associationAsSource.1">
    <atributes
        name="nombre"
        dataType="string">
      <validations/>
      <validations
          validationType="uniqueness"/>
    </atributes>
    <atributes
        name="apellido"
        dataType="string">
      <validations/>
    </atributes>
    <atributes
        name="fechaNacimiento"
        dataType="date"
        isFilter="false"/>
    <crud>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.0/@atributes.0"/>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.0/@atributes.1"/>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.0/@atributes.2"/>
      <fields
          xsi:type="crudGenerator:AssociationField"
          component="selectListMultiple"
          association="//@entities.0/@associationAsSource.0"/>
    </crud>
    <associationAsSource
        sourceName="estudiantes"
        targetName="cursos"
        manySource="true"
        manyTarget="true"
        target="//@entities.1"/>
  </entities>
  <entities
      name="Curso"
      associationAsTarget="//@entities.0/@associationAsSource.0">
    <atributes
        name="nombre"
        dataType="string">
      <validations/>
    </atributes>
    <atributes
        name="codigo"
        dataType="string"
        isFilter="false">
      <validations/>
      <validations
          validationType="uniqueness"/>
    </atributes>
    <atributes
        name="descripcion"
        dataType="text"
        isFilter="false"/>
    <crud>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.1/@atributes.0"/>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.1/@atributes.1"/>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.1/@atributes.2"/>
      <fields
          xsi:type="crudGenerator:AssociationField"
          association="//@entities.1/@associationAsSource.0"/>
      <fields
          xsi:type="crudGenerator:AssociationField"
          component="autocompleteMultiple"
          association="//@entities.1/@associationAsSource.1"/>
    </crud>
    <associationAsSource
        sourceName="cursos"
        targetName="periodoacademico"
        manySource="true"
        target="//@entities.2"/>
    <associationAsSource
        sourceName="curso"
        targetName="estudiantes"
        manySource="true"
        manyTarget="true"
        target="//@entities.0"/>
  </entities>
  <entities
      name="PeriodoAcademico"
      associationAsTarget="//@entities.1/@associationAsSource.0">
    <atributes
        name="nombre"
        dataType="string">
      <validations/>
    </atributes>
    <atributes
        name="fechaInicio"
        dataType="date"
        isFilter="false">
      <validations/>
    </atributes>
    <atributes
        name="fechaFin"
        dataType="date"
        isFilter="false">
      <validations/>
    </atributes>
    <crud>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.2/@atributes.0"/>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.2/@atributes.1"/>
      <fields
          xsi:type="crudGenerator:PrimitiveField"
          attribute="//@entities.2/@atributes.2"/>
    </crud>
  </entities>
</crudGenerator:Application>
