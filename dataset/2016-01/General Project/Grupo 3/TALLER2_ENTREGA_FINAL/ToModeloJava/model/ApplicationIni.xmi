<?xml version="1.0" encoding="UTF-8"?>
<toModeloJava:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:toModeloJava="http://www.example.org/toModeloJava">
  <packages
      name="marketPlace">
    <classes
        name="Producto">
      <attributes
          name="id"
          type="integer"/>
      <attributes
          name="name"/>
      <methods
          name="calculaPrecio"
          returnType="double">
        <parameter
            name="cantidad"
            type="double"/>
        <parameter
            name="iva"/>
        <annotations
            name="Transactional"/>
      </methods>
      <annotations
          name="Entity"/>
    </classes>
    <classes
        name="Carrito">
      <attributes
          name="id"
          type="integer"/>
      <attributes
          name="name"/>
      <annotations
          name="Entity"/>
    </classes>
    <classes
        name="Cliente">
      <attributes
          name="id"
          type="integer"/>
      <attributes
          name="name"/>
      <attributes
          name="password"/>
      <annotations
          name="Entity"/>
    </classes>
    <classes
        name="CarritoItem">
      <annotations
          name="Entity"/>
    </classes>
    <classes
        name="Item">
      <attributes
          name="id"
          type="integer"/>
      <attributes
          name="name"/>
      <attributes
          name="cantidad"
          type="double"/>
      <annotations
          name="Entity"/>
    </classes>
  </packages>
</toModeloJava:Application>
