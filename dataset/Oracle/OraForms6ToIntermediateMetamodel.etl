	n.orderbyClause = t.blockDatabase.ORDERBYClause;
	n.insertAllowed = t.blockDatabase.insertAllowed;
	n.updateAllowed = t.blockDatabase.updateAllowed;
	n.deleteAllowed = t.blockDatabase.deleteAllowed;
