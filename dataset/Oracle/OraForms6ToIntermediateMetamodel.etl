		if(isValidItem(itm)){
					
			//itm.equivalent('ItemTextItemToBasicAttribute').name.println();
			//n.attributes.add(itm.equivalent('ItemTextItemToBasicAttribute'));
			addDisplayFieldToDataDefinition(itm, n);
			
