	for(tr2 in t.eContainer().triggers){
		addTriggerToDataDefinition(n, tr2, '',t.name.toLowerCase());
	}
	for(trg in t.triggers){
	
		addTriggerToDataDefinition(n, trg, '',t.name.toLowerCase());
	}
