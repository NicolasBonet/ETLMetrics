<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="androidModel" nsURI="http://www.example.org/androidModel" nsPrefix="androidModel">
  <eClassifiers xsi:type="ecore:EClass" name="AndroidApp">
    <eStructuralFeatures xsi:type="ecore:EReference" name="categories" eType="#//StringArray"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="languages" eType="#//StringArray"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fonts" eType="#//StringArray"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sizes" eType="#//StringArray"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StringArray">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="items" upperBound="-1"
        eType="#//Item" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Item">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subcategories" eType="#//StringArray"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="articulo" eType="#//StringArray"
        containment="true"/>
  </eClassifiers>
</ecore:EPackage>
